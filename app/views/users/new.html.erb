<DOCTYPE! html>
<html>
    <head>
       <title>User Management</title>
    </head>
    <body>
        <form action='/create_process' method='post'>
            <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
            <p>First Name: <input type="text" name="form[first_name]"></p>
            <p>Last Name: <input type="text" name="form[last_name]"></p>
            <p>Email Address: <input type="text" name="form[email_address]"></p>
            <p>Password: <input type="password" name="form[password]"></p>
            <input type='submit' name='create_process' value='Create'/>
        </form>
        <% if flash[:errors] %>
            <% flash[:errors].each do |i| %>
                <ul style="list-style: none;">
                    <li><%= i %></li>
                </ul>
            <% end %>
        <% end %>
        <a href="/back">Back</a>
    </body>
</html>
